		<queryString>
			<![CDATA[select empleado.idempleado, concat(empleado.apellido, ', ' ,empleado.nombre) as empleado, empleado.dni, empleado.telefono, date_format(empleado.fechaIngreso,'%d/%m/%Y') as fechaIngreso,concat( (TIMESTAMPDIFF(YEAR, empleado.fechaIngreso, CURDATE())), ' AÃ±os') As antiguedad, empleado.sexo, empleado.estado, 
concat(domicilio.calle, ' ' ,domicilio.numero) as calle, domicilio.piso, domicilio.departamento,
cargo.cargo, cargo.basico
from empleado
inner join domicilio on domicilio.iddomicilio = empleado.domicilio_iddomicilio
inner join cargo on cargo.idcargo = empleado.cargo_idcargo;]]>
